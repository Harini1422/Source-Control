    "scriptContent" : "sys.path.insert(0,AVX::DEPENDENCIES)\nsys.path.insert(0,AVX::HELPER)\nnull = None\n\nfrom appviewx_sdk.f5.bigip import Management\n\n\n\nVIRTUAL_SERVER_NAME = \"<%virtual_server_name%>\"\nADDRESS_LIST = \"<%address_list%>\"\nGTM_SERVER_NAME = \"<%server_name%>\"\nDATA_CENTRE = \"<%datacentre%>\"\n\ncommands = <%commands%>\n\n\nmgmt = Management('<%device_name%>', '<%sessionId%>')\nmgmt.get_tmsh()\n\nserver_payload = {'name': GTM_SERVER_NAME,\"virtual_server_name\":VIRTUAL_SERVER_NAME,\"address_list\": ADDRESS_LIST,\"addresses\":ADDRESS_LIST,\"datacentre\":DATA_CENTRE}\n\nmgmt.tm.gtm.server.bigip.create(server_payload)\n\nx = mgmt.get_commands()\n\nout = mgmt.merge_config(commands,x)\n\nAVX::OUTPUT(out)\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
