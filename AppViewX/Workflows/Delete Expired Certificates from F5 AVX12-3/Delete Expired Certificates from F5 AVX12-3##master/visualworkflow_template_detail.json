    "scriptType" : "Python",
    "scriptContent" : "import sys\nimport datetime\nimport json\n\nsys.path.insert(0, AVX::DEPENDENCIES)\nsys.path.insert(0, AVX::HELPER)\nimport appviewx\nimport Decrypt_Python3 as Decrypt\nimport certificate_helper_vw as certificate_helper\nimport paramiko\n\n# Global variable and template form \nf5device =\"<%createform_1.output.f5device%>\"\nexpired_cert = \"<%createform_1.output.expired_cert%>\"\n\ncert_name_cmd=[]\nkey_name_cmd=[]\nprofile_modify_cmd=[]\n\n# Get f5 device version\nconnection = appviewx.db_connection()\ncollection = connection.appviewx.device\ndev_version = collection.find_one({'name':f5device})['version']\n\ndevice_ip = collection.find_one({'name':f5device})['ip']\nusername, password = Decrypt.getpassword(f5device)\n\ndef connect_remote(IP,username,password):\n    try:\n        remote_conn=paramiko.SSHClient()\n        remote_conn.set_missing_host_key_policy(paramiko.AutoAddPolicy())\n        remote_conn.connect(IP,username=username,password=password,look_for_keys=False,allow_agent=False)\n        return remote_conn\n    except Exception as e:\n        print (str(e))\n        \ndef check_bash_mode(connection) :\n    stdin, stdout, err=connection.exec_command('echo \"testing\"')\n    err = err.read().decode()\n    if \"Error\" not in err:\n        return True\n    else:\n        return False    \n        \ndef generate_delete_modify_command(bash_mode):\n    \"\"\"\n    This function gemerate lis of modify profile and delete expired cert and key command.\n    Args:\n       None\n    Returns:\n       None\n    \"\"\"\n    # Get list of cert and key from f5 device\n    cert_name_list = certificate_helper.get_list_of_cert_name(f5device)\n    key_name_list = certificate_helper.get_list_of_key_name(f5device)\n    \n    # Get list of client and server profile\n    client_ssl=certificate_helper.get_ssl_client_profile(f5device)\n    server_ssl=certificate_helper.get_ssl_server_profile(f5device)\n\n    cert_list = expired_cert.split(\",\")\n    # Get expired cert name and associated profile with expeired key\n    dict_cert_profile = certificate_helper.get_profile_of_exp_cert(cert_list, f5device)\n    \n    for name, profile in dict_cert_profile.items():\n        # If profile has expired cert, below step remove old cert and assign default cert and key\n        if len(profile):\n            for profile_name in profile:                \n                profile_tpe = \"client-ssl\" if profile_name.strip() in client_ssl else \"server-ssl\" \n                cmd=\"modify ltm profile {0} {1} cert default.crt key default.key\".format(profile_tpe, profile_name)\n                profile_modify_cmd.append(cmd)\n        \n        # Below steps to generate expired cert and key deletion command\n        if dev_version in [\"v11\",\"v12\"]:\n            cert_name_cmd.append(\"delete sys crypto cert \"+name)\n            if name.replace(\".crt\",\".key\") in key_name_list:\n                key_name_cmd.append(\"delete sys crypto key \"+name.replace(\".crt\",\".key\")) \n        else:\n            if name.strip() in [cert_name[\"cert_name\"] for cert_name in cert_name_list]:\n                cert_name_cmd.append(\"run util bash -c 'rm /config/ssl/ssl.crt/{0}'\".format(name))\n            if name.replace(\".crt\",\".key\") in key_name_list:\n                key_name_cmd.append(\"run util bash -c 'rm /config/ssl/ssl.key/{0}'\".format(name.replace(\".crt\",\".key\")))  \n                \nif  __name__ == '__main__':\n    connection = connect_remote(device_ip, username, password)\n    bash_mode = check_bash_mode(connection)\n    generate_delete_modify_command(bash_mode)\n    if bash_mode:\n        AVX::CMD(f5device +':@'+ 'tmsh')\n    if len(profile_modify_cmd) and dev_version in [\"v11\",\"v12\"]:\n        AVX::CMD(f5device +':@'+ 'create cli transaction')\n        profile_modify_cmd = list(set(profile_modify_cmd))\n    for cmnd in profile_modify_cmd:\n        AVX::CMD(f5device +':@'+ cmnd)\n        \n    if len(profile_modify_cmd) and dev_version in [\"v11\",\"v12\"]:\n        AVX::CMD(f5device +':@'+ 'submit cli transaction')    \n    for cmnd in cert_name_cmd:\n        AVX::CMD(f5device +':@'+ cmnd)\n    for cmnd in key_name_cmd:    \n        AVX::CMD(f5device +':@'+ cmnd)\n    if len(profile_modify_cmd) or dev_version in [\"v11\",\"v12\"]:\n        AVX::CMD(f5device +':@'+ 'save sys config')\n    if bash_mode:\n        AVX::CMD(f5device +':@'+ 'quit')\n    AVX::LOG(\"Implementation compiled\")    \n    config = {}\n    config['default_imp'] = AVX::CONFIG\n    AVX::OUTPUT(config)\n",
